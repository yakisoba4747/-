#include <stdio.h>

int main()
{
    int k;
    printf("How many pockets?");
    scanf("%d", &k);
    
    printf("The number of pockets is %d", k);
    
    
    int p;
     printf("How many biscuits?");
    scanf("%d", &p);
    
    printf("The number of biscuits is %d", p);
    
    int sum;
    sum=k+p;
     printf("The number of pockets and biscuits: %d\n",sum);
    
    
    



    return 0;
